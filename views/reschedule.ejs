<%- include('partials/header') %>
</section>

<div class="container appointment">
    <div class="jumbotron centered">
        <h1 class="display-5">Reschedule an Appointment</h1>
        <div class="info">
            <p><strong>Info!</strong> You can only update Appointment Date.</p>
        </div>

        <form action="/reschedule" method="POST" enctype="multipart/form-data">

            <p><strong>Schedule Information</strong></p>
            <hr>

            <div class="form-row">
                <div class="form-group col-md-4">
                    <label for="inputPassword4">Appointment Date</label>
                    <input type="date" class="form-control" id="inputPassword4" name="sDate" placeholder="Password">
                </div>
            </div>
            <button type="submit" class="btn btn-primary">Update</button>
        </form>


    </div>
</div>
<%- include('partials/footer') %>
